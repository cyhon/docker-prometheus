synapse_http_server_requests:method{servlet=""} = sum(synapse_http_server_requests) by (method)
synapse_http_server_requests:servlet{method=""} = sum(synapse_http_server_requests) by (servlet)
synapse_http_server_requests:total{servlet=""} = sum(synapse_http_server_requests:method) by (servlet)

ALERT SynapseHTTPServerQPS
  IF rate(synapse_http_server_requests:total[1m]) > 100
  FOR 1m
  ANNOTATIONS {
    summary = "{{ $labels.instance }} {{ $labels.alertname  }}",
    description = "{{ $labels.instance }} QPS过高(当前值: {{ $value }}req/s)",
  }
